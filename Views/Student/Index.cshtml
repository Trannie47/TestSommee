@model List<TestSommee.Models.Student>

<h2>Danh sách sinh viên</h2>

<table border="1" cellpadding="8">
    <tr>
        <th>ID </th>
        <th>Tên </th>
        <th>Email </th>
    </tr>
@foreach (var s in Model)
{
    <tr>
        <td>@s.Id</td>
        <td>@s.Name</td>
        <td>@s.Email</td>
    </tr>
}
</table>
